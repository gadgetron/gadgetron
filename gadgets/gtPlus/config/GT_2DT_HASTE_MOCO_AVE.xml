    <!-- reader -->
    <reader>
        <slot>1008</slot>
        <dll>gadgetron_mricore</dll>
        <classname>GadgetIsmrmrdAcquisitionMessageReader</classname>
    </reader>

    <!-- writer -->
    <writer>
        <slot>1022</slot>
        <dll>gadgetron_mricore</dll>
        <classname>MRIImageWriter</classname>
    </writer>

    <!--
    <writer>
        <slot>1012</slot>
        <dll>gadgetron_dicom</dll>
