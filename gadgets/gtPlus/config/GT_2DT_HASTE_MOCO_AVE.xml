             ISMRMRD_FILTER_GAUSSIAN,
             ISMRMRD_FILTER_HANNING,
             ISMRMRD_FILTER_TUKEY,
             ISMRMRD_FILTER_TAPERED_HANNING,
             ISMRMRD_FILTER_NONE 
