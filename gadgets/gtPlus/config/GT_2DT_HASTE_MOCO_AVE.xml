        </property>
        <property>
            <name>filterE1_width</name>
            <value>0.15</value>
        </property>

        <property>
            <name>filterE2</name>
            <value>ISMRMRD_FILTER_GAUSSIAN</value>
        </property>
        <property>
            <name>filterE2_sigma</name>
            <value>1.0</value>
        </property>
        <property>
            <name>filterE2_width</name>
            <value>0.15</value>
        </property>

        <!-- parameters for kspace filter, ref data -->
