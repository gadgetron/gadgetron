        </property>

        <property>
            <name>upstream_coil_compression</name>
            <value>false</value>
        </property>

        <property>
            <name>upstream_coil_compression_thres</name>
            <value>-1</value>
        </property>

        <property>
            <name>upstream_coil_compression_num_modesKept</name>
            <value>-1</value>
        </property>

        <property>
            <name>downstream_coil_compression</name>
            <value>true</value>
